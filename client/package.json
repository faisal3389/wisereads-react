    "react-native-numeric-input": "^1.3.1",
    "react-native-simple-dialogs": "^0.4.1",
    "react-native-splash-screen": "^3.1.1",
    "react-native-vector-icons": "^5.0.0",
    "react-navigation": "^2.9.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
